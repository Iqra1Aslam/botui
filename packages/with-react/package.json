{
  "name": "@botui/react",
  "version": "1.0.0-alpha",
  "description": "Create the UI of your bot with react",
  "main": "src/index.ts",
  "files": [
    "src"
  ],
  "scripts": {
    "link": "npm link ./",
    "unlink": "npm unlink ./",
    "danger-deploy": "npm publish",
    "link-botui": "npm link botui",
    "unlink-botui": "npm unlink botui",
    "build": "npm run build-scss && tsc -p tsconfig.json",
    "dev": "parcel --no-cache local-react-test/index.html --dist-dir local-dist",
    "build-scss": "npx typed-scss-modules src/styles/ -n none --exportType default"
  },
  "keywords": [
    "botui",
    "react",
    "conversational",
    "form",
    "interface"
  ],
  "author": {
    "name": "Moin Uddin",
    "url": "https://moin.im"
  },
  "license": "MIT",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.7.0",
    "@types/react": "^18.0.20",
    "parcel": "^2.7.0",
    "process": "^0.11.10",
    "typed-scss-modules": "^7.0.0",
    "typescript": "^4.8.3"
  },
  "peerDependencies": {
    "botui": "^1.0.0-alpha",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
