{
  "name": "@botui/react",
  "version": "1.0.0-alpha",
  "description": "Create the UI of your bot with react",
  "main": "dist/index.js",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "link": "npm link ./",
    "unlink": "npm unlink ./",
    "danger-deploy": "npm publish",
    "link-botui": "npm link botui",
    "unlink-botui": "npm unlink botui",
    "copy-styles": "cp -r src/styles dist/styles",
    "build": "npm run build-scss && tsc -p tsconfig.json && npm run copy-styles",
    "build-scss": "npx typed-scss-modules src/styles/ -n none --exportType default"
  },
  "keywords": [
    "botui",
    "react",
    "conversational",
    "form",
    "interface"
  ],
  "author": {
    "name": "Moin Uddin",
    "url": "https://moin.im"
  },
  "license": "MIT",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.7.0",
    "@types/react": "^18.0.20",
    "node-sass": "^7.0.3",
    "parcel": "^2.7.0",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typed-scss-modules": "^7.0.0",
    "typescript": "^4.8.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
